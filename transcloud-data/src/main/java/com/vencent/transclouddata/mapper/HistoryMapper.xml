<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vencent.transclouddata.mapper.HistoryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.vencent.transclouddata.entity.History">
        <id column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="trans_source" property="transSource" />
        <result column="trans_target" property="transTarget" />
        <result column="trans_speech" property="transSpeech" />
        <result column="trans_time" property="transTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, trans_source, trans_target, trans_speech, trans_time
    </sql>

</mapper>
